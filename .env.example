APP_KEY=                          #php artisan make:auth  before database migration
APP_DEBUG=true                    #false in production environment  #true in local environment
APP_URL=https://laravel.app

LOG_CHANNEL=stack                 # config logging.php

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_TIMEZONE=UTC
APP_ADMIN_PATH=admin

DATATABLES_ERROR = "Please change input ?"

DB_CONNECTION=mysql
DB_HOST=192.168.10.10             #homestead
DB_PORT=3306
DB_DATABASE= laravel              #create
DB_USERNAME=homestead
DB_PASSWORD=secret

BROADCAST_DRIVER=log
CACHE_DRIVER=file
SESSION_DRIVER=database            #file or database #https://laravel.com/docs/5.6/session
SESSION_LIFETIME=120
QUEUE_DRIVER=sync

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_DRIVER=smtp                   #google
MAIL_FROM_ADDRESS = xxxxxx         #mail from email
