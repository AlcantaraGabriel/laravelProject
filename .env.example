APP_NAME=
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

DB_HOST=db
DB_DATABASE=local

MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=
MAIL_PASSWORD=

# Turns the frontend template routes on and off.
FRONTEND_ENABLED=true

# Tells docker-compose which file to use by default, and what prefix to give the
# containers it created. If using docker in production, create a new
# `docker/production.docker-compose.yml etc and change this variable.
COMPOSE_FILE=docker/local.docker-compose.yml
COMPOSE_PROJECT_NAME=

# Only set this in environments you want to report errors, i.e. production.
SENTRY_LARAVEL_DSN=
