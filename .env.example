MAIL_PORT=587                      #google
MAIL_USERNAME = xxxxx              #your gmail addres
MAIL_PASSWORD = xxxxxx             #mje   --> https://myaccount.google.com/apppasswords
MAIL_ENCRYPTION=tls                #google

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
